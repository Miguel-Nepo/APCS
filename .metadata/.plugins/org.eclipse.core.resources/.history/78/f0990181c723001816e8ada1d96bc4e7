import java.io.File;
import java.io.IOException;
import java.util.Scanner;
import java.util.ArrayList;


public class Runner
{
	private ArrayList<Integer> count;
	private static ArrayList<Character> letters;

	public static void main( String args[] ) throws IOException
	{
		Scanner file = new Scanner(new File("lab16e1.dat"));
			while (file.hasNext()){
				for (int i = 0;i<file.next().length();i++){
					if (file.next().charAt(i)=='a'){
						System.out.println("*"+file.next().charAt(i));
					}
					else{
						System.out.println(file.next().charAt(i));
					}
				}
			}

		//more test cases
	}

}