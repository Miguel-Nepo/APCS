import static java.lang.System.*;

public class Lab09f
{
	public static void main( String args[] )
	{
		LetterRemover remover1 = new LetterRemover("I am Sam I am", 'a');
		remover1.removeLetters();
		out.println(remover1);
		
		LetterRemover remover2 = new LetterRemover("ssssssssxssssesssssesss", 's');
		remover2.removeLetters();
		out.println(remover2);
		
		LetterRemover remover3 = new LetterRemover("qwertyqwertyqwerty", 'a');
		remover3.removeLetters();
		out.println(remover3);
		
		LetterRemover remover4 = new LetterRemover("abaababababa", 'x');
		remover4.removeLetters();
		out.println(remover4);
		
		
											
	}
}